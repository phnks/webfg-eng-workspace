# Discord User ID of the admin/recipient for DMs from the bots
ADMIN_DISCORD_ID=YOUR_DISCORD_USER_ID_HERE

# Bot tokens for each developer VM user
# Add one line for each user listed in config/dev_users.txt
BOT_TOKEN_testuser=TOKEN_FOR_TESTUSER_BOT_HERE
BOT_TOKEN_anum=TOKEN_FOR_ANUM_BOT_HERE
# BOT_TOKEN_anotheruser=TOKEN_FOR_ANOTHERUSER_BOT_HERE

GIT_USERNAME=
GIT_TOKEN=
GH_TOKEN=

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
